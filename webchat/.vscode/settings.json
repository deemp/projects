{
    "todo-tree.regex.regex": "((--\\s*($TAGS))|\\{-\\s($TAGS).*(\\n.*)*-})",
    "haskell.serverExecutablePath": "haskell-language-server",
    "files.watcherExclude": {
        "**/.spago/**": true
    },
    "purescript.outputDirectory": "./frontend/output/",
    "purescript.packagePath": "./frontend",
    "purescript.sourcePath": "./frontend/src",
    "purescript.buildCommand": "(cd frontend && spago build --purs-args --json-errors)",
    "vscode-dhall-lsp-server.executable": "dhall-lsp-server"
}